class HelloWorld {
public static void main(String[] args) {
        int n = 5;
        for(int i=0;i<=n;i++){
            if(i<2){
                System.out.print(i+", ");
            }else{
                int fibo = (i-2)+(i-1);
            System.out.print(fibo+", ");
            }
        }
    }
}
